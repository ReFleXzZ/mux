{
    "name": "mux",
    "displayName": "mux",
    "description": "Manage tmux from the safety of vscode",
    "version": "0.4.1",
    "publisher": "elken",
    "engines": {
        "vscode": "^1.16.0"
    },
    "preview": true,
    "license": "MIT",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:.mux.json"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/elken/mux"
    },
    "keywords": [
        "tmux",
        "terminal",
        "multiplexer"
    ],
    "author": {
        "name": "Ellis Kenyo",
        "email": "elken@mykolab.com",
        "url": "https://elken.me"
    },
    "bugs": {
        "url": "https://github.com/elken/mux/issues"
    },
    "homepage": "https://elken.github.io/mux/",
    "icon": ".github/mux.png",
    "galleryBanner": {
        "color": "#000000",
        "theme": "dark"
    },
    "main": "./out/src/extension",
    "contributes": {
        "keybindings": [
            {
                "command": "extension.moveLast",
                "key": "win+alt+shift+1"
            },
            {
                "command": "extension.moveNext",
                "key": "win+alt+."
            },
            {
                "command": "extension.movePrevious",
                "key": "win+alt+,"
            },
            {
                "command": "extension.showMux",
                "key": "alt+m s"
            },
            {
                "command": "extension.killCurrentSession",
                "key": "alt+m k"
            },
            {
                "command": "extension.killCurrentSession",
                "key": "alt+m shift+k"
            },
            {
                "command": "extension.moveLeft",
                "key": "alt+m left"
            },
            {
                "command": "extension.moveRight",
                "key": "alt+m right"
            },
            {
                "command": "extension.moveUp",
                "key": "alt+m up"
            },
            {
                "command": "extension.moveDown",
                "key": "alt+m down"
            }
        ],
        "commands": [
            {
                "command": "extension.showMux",
                "title": "Show Mux",
                "category": "Mux"
            },
            {
                "command": "extension.killSessions",
                "title": "Kill mux sessions using current prefix",
                "category": "Mux"
            },
            {
                "command": "extension.killCurrentSession",
                "title": "Kill mux session for current project",
                "category": "Mux"
            },
            {
                "command": "extension.configureSettings",
                "title": "Configure Mux",
                "category": "Mux"
            },
            {
                "command": "extension.moveLast",
                "title": "Move to last active pane",
                "category": "Mux"
            },
            {
                "command": "extension.moveNext",
                "title": "Move to next clockwise pane",
                "category": "Mux"
            },
            {
                "command": "extension.movePrevious",
                "title": "Move to next counter-clockwise pane",
                "category": "Mux"
            },
            {
                "command": "extension.moveTopmost",
                "title": "Move to the uppermost pane",
                "category": "Mux"
            },
            {
                "command": "extension.moveBottommost",
                "title": "Move to the bottommost pane",
                "category": "Mux"
            },
            {
                "command": "extension.moveLeftmost",
                "title": "Move to the leftmost pane",
                "category": "Mux"
            },
            {
                "command": "extension.moveRightmost",
                "title": "Move to the rightmost pane",
                "category": "Mux"
            },
            {
                "command": "extension.moveTopLeftmost",
                "title": "Move to the top-leftmost pane",
                "category": "Mux"
            },
            {
                "command": "extension.moveTopRightmost",
                "title": "Move to the top-rightmost pane",
                "category": "Mux"
            },
            {
                "command": "extension.moveBottomLeftmost",
                "title": "Move to the bottom-leftmost pane",
                "category": "Mux"
            },
            {
                "command": "extension.moveBottomRightmost",
                "title": "Move to the bottom-rightmost pane",
                "category": "Mux"
            },
            {
                "command": "extension.moveUp",
                "title": "Move up 1 pane",
                "category": "Mux"
            },
            {
                "command": "extension.moveDown",
                "title": "Move down 1 pane",
                "category": "Mux"
            },
            {
                "command": "extension.moveLeft",
                "title": "Move left 1 pane",
                "category": "Mux"
            },
            {
                "command": "extension.moveRight",
                "title": "Move right 1 pane",
                "category": "Mux"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Mux Configuration",
            "properties": {
                "mux.tmuxPath": {
                    "type": "string",
                    "default": "tmux",
                    "description": "Path to tmux executable"
                },
                "mux.globalConfiguration": {
                    "type": "object",
                    "default": {},
                    "description": "The global mux configuration which will get loaded if no project configuration is found"
                },
                "mux.prefix": {
                    "type": "string",
                    "default": "vsc",
                    "description": "String to prepend to session names to differentiate over normal tmux sessions"
                },
                "mux.runAtStartup": {
                    "type": "boolean",
                    "default": false,
                    "description": "Whether or not to run the mux session at startup"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "doc": "typedoc --exclude '**/+(index|*.test).ts' . --out docs/ "
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "enum-values": "^1.2.0",
        "jsonschema": "^1.2.0",
        "lodash.indexof": "^4.0.5",
        "mocha": "^3.5.0",
        "typescript": "^2.5.2",
        "vscode": "^1.1.5"
    },
    "dependencies": {
        "lodash": "^4.17.4",
        "object-hash": "^1.1.8",
        "util.promisify": "^1.0.0"
    }
}